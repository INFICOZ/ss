local Tab = Window:NewTab("EPS") 
local Section = Tab:NewSection("บอกตำแหน่ง")

Section:NewButton("EPS WHITE", "EPS", function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

-- ฟังก์ชันสร้าง ESP สีขาว
local function createESP(player)
    if player == LocalPlayer then return end -- ไม่แสดง ESP บนตัวเอง

    local function applyESP(character)
        local rootPart = character:FindFirstChild("HumanoidRootPart")
        if not rootPart then return end

        -- ลบ ESP เดิมก่อนเพิ่มใหม่
        if rootPart:FindFirstChild("ESP") then
            rootPart.ESP:Destroy()
        end

        -- สร้าง BillboardGui สำหรับ ESP
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP"
        billboard.Adornee = rootPart
        billboard.Size = UDim2.new(3, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.TextColor3 = Color3.new(1, 1, 1) -- สีขาว
        textLabel.TextStrokeTransparency = 0.5
        textLabel.TextScaled = true
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = rootPart

        -- อัปเดตระยะทางตลอดเวลา
        RunService.RenderStepped:Connect(function()
            if character and rootPart and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local distance = (LocalPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
                textLabel.Text = string.format("%s\n%.1f studs", player.Name, distance)
            end
        end)
    end

    -- ตรวจสอบว่า Player มี Character แล้วหรือไม่
    if player.Character then
        applyESP(player.Character)
    end

    -- ติดตามเมื่อ Player Spawn ตัวละครใหม่
    player.CharacterAdded:Connect(applyESP)
end

-- เพิ่ม ESP ให้กับผู้เล่นที่มีอยู่แล้ว
for _, player in pairs(Players:GetPlayers()) do
    createESP(player)
end

-- ติดตามการเข้ามาของผู้เล่นใหม่
Players.PlayerAdded:Connect(createESP)
end)

Section:NewButton("EPS Rainbow", "EPS", function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local function createRainbowESP(player)
    if player == game.Players.LocalPlayer then return end -- ไม่แสดง ESP บนตัวเอง

    local function applyRainbowESP(character)
        if not character or not character:FindFirstChild("HumanoidRootPart") then return end

        -- ลบ Highlight เดิมก่อนเพิ่มใหม่
        for _, part in pairs(character:GetChildren()) do
            if part:IsA("Part") or part:IsA("MeshPart") then
                if part:FindFirstChild("Highlight") then
                    part.Highlight:Destroy()
                end

                -- เพิ่ม Highlight ให้ทุกๆ Part
                local highlight = Instance.new("Highlight")
                highlight.Parent = part
                highlight.FillTransparency = 0.5  -- ทำให้มองทะลุ
                highlight.OutlineTransparency = 0
                highlight.FillColor = Color3.fromRGB(255, 0, 0)  -- เริ่มต้นสี
                highlight.OutlineColor = Color3.fromRGB(255, 0, 0)  -- เริ่มต้นสี outline

                -- ฟังก์ชันเปลี่ยนสีแบบสีรุ้ง
                local function applyRainbowColor()
                    local time = tick()
                    local color = Color3.fromRGB(
                        math.abs(math.sin(time)) * 255,
                        math.abs(math.sin(time + 2)) * 255,
                        math.abs(math.sin(time + 4)) * 255
                    )
                    
                    -- เปลี่ยนสีของ Highlight
                    highlight.FillColor = color
                    highlight.OutlineColor = color
                end

                -- เชื่อมต่อการอัปเดตสีกับ RenderStepped
                RunService.RenderStepped:Connect(applyRainbowColor)
            end
        end
    end

    -- ถ้าผู้เล่นมีตัวละคร, ให้ใช้ฟังก์ชัน
    if player.Character then
        applyRainbowESP(player.Character)
    end

    player.CharacterAdded:Connect(applyRainbowESP)
end

-- เพิ่ม ESP สีรุ้งให้ผู้เล่นที่มีอยู่แล้ว
for _, player in ipairs(Players:GetPlayers()) do
    createRainbowESP(player)
end

-- เชื่อมต่อการเพิ่มผู้เล่นใหม่
Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        createRainbowESP(player)
    end)
end)
end)

