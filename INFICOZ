local Tab = Window:NewTab("Bypass") 
local Section = Tab:NewSection("บายพาส")
Section:NewButton("Bypass", "เอาไว้เปิดเมนู Admin", function()
local getinfo = getinfo or debug.getinfo
local DEBUG = false
local Hooked = {}

local Detected, Kill

setthreadidentity(2)

for i, v in getgc(true) do
    if typeof(v) == "table" then
        local DetectFunc = rawget(v, "Detected")
        local KillFunc = rawget(v, "Kill")
    
        if typeof(DetectFunc) == "function" and not Detected then
            Detected = DetectFunc
            
            local Old; Old = hookfunction(Detected, function(Action, Info, NoCrash)
                if Action ~= "_" then
                    if DEBUG then
                        warn(`Adonis AntiCheat flagged\nMethod: {Action}\nInfo: {Info}`)
                    end
                end
                
                return true
            end)

            table.insert(Hooked, Detected)
        end

        if rawget(v, "Variables") and rawget(v, "Process") and typeof(KillFunc) == "function" and not Kill then
            Kill = KillFunc
            local Old; Old = hookfunction(Kill, function(Info)
                if DEBUG then
                    warn(`Adonis AntiCheat tried to kill (fallback): {Info}`)
                end
            end)

            table.insert(Hooked, Kill)
        end
    end
end

local Old; Old = hookfunction(getrenv().debug.info, newcclosure(function(...)
    local LevelOrFunc, Info = ...

    if Detected and LevelOrFunc == Detected then
        if DEBUG then
            warn(`zins | adonis bypassed`)
        end

        return coroutine.yield(coroutine.running())
    end
    
    return Old(...)
end))
-- setthreadidentity(9)
setthreadidentity(7)
end)
Section:NewButton("KILL ALL", "กูบอกอย่ากด", function()
    local player = game.Players.LocalPlayer

    -- แสดงการแจ้งเตือน
    game.StarterGui:SetCore("SendNotification", {
        Title = "โปรแกรมกำลัง",
        Text = "ได้เฉพาะบางแมพ!",
        Icon = "rbxassetid://1234567890", -- คุณสามารถใส่ ID ของไอคอนที่ต้องการได้
        Duration = 5
    })
    
    -- รอ 10 วินาที
    wait(10)

    -- เตะผู้เล่นออกจากเกม พร้อมข้อความที่ต้องการ
    player:Kick("")
end)
